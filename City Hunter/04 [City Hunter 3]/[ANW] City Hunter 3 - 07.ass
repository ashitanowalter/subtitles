Dialogue: 0,0:00:34.40,0:00:38.66,City Hunter 2 (Bot),,0,0,0,,
Dialogue: 0,0:00:39.90,0:00:43.87,City Hunter 2 (Bot),,0,0,0,,
Dialogue: 0,0:00:43.87,0:00:47.68,City Hunter 2 (Bot),,0,0,0,,
